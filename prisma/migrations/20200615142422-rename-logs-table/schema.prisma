datasource db {
  provider = "postgresql"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id String @default(cuid()) @id
  name String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Log {
  id String @default(cuid()) @id
  type String
  name String
  description String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
